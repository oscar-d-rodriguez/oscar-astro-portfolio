---
import Welcome from "@components/Welcome/Welcome.astro";
import About from "@components/About/About.astro";
import Projects from "@components/Projects/Projects.astro";
import Contact from "@components/Contact/Contact.astro";
import Layout from "@layouts/Layout.astro";

const response = await fetch(
   "https://8dcsc1l247.execute-api.us-east-2.amazonaws.com/dev/astro-api",
);
const data = await response.json();
---

<Layout>
   <Welcome data={data} />
   <About data={data} />
   <Projects />
   <Contact data={data} />
</Layout>
